{
  "name": "alphax",
  "version": "0.0.6",
  "description": "my beautiful project",
  "repository": {
    "url": "ULIVZ/alphaX",
    "type": "git"
  },
  "main": "dist/alphax.js",
  "files": [
    "src",
    "dist",
    "index.d.ts"
  ],
  "scripts": {
    "test:cov": "npm run build && jest --coverage && npm run lint",
    "test": "npm run lint && jest",
    "lint": "xo",
    "prepublish": "npm run build",
    "build": "bili"
  },
  "author": "ULIVZ <chl814@foxmail.com>",
  "license": "MIT",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "bili": {
    "input": "src/index.ts",
    "js": "typescript",
    "filename": "alphax.js"
  },
  "babel": {
    "env": {
      "test": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "node": "current"
              }
            }
          ]
        ]
      }
    }
  },
  "dependencies": {
    "event-stream": "^3.3.4",
    "fs-extra": "^3.0.1",
    "map-stream": "0.0.7",
    "vinyl-fs": "^3.0.1",
    "ware": "^1.3.0"
  },
  "devDependencies": {
    "@types/event-stream": "^3.3.33",
    "@types/fs-extra": "^5.0.0",
    "@types/jest": "^22.0.1",
    "@types/node": "^8.5.7",
    "@types/vinyl-fs": "^2.4.8",
    "babel-preset-env": "^1.4.0",
    "bili": "^1.3.8",
    "eslint-config-rem": "^3.0.0",
    "jest-cli": "^22.0.4",
    "rollup-plugin-typescript": "^0.8.1",
    "ts-jest": "^22.0.1",
    "typescript": "^2.6.2",
    "xo": "^0.18.0"
  },
  "typings": "index.d.ts",
  "xo": {
    "extends": "rem",
    "esnext": true,
    "envs": [
      "jest"
    ],
    "ignores": [
      "test/fixture/**",
      "test/output/**",
      "examples/**"
    ],
    "rules": {
      "guard-for-in": 0
    }
  }
}
